{
  "AppSettings": {
    "MockConfigFilePath": "LogicConfigFile.Xml",
    "syslogHostName": "127.0.0.1",
    "syslogPortNumber": 514
  },
  "LogSettings": {

    "LogGlobalConfiguration": {
      //In Milliseconds
      "TimeUntilFileWrite": 10000,
      "MaxEntriesInDictionary": 100,
      /*
      In Case that one of the dictionary is reached max entries 
      flush all dictionary or only specific
      */
      "FlushAllDictionaryAtOnce": true
    },
    "LogAll": {
      "Implemented": false
    },
    "LogDebug": {
      "Implemented": true,
      "MemorySize": 5000,
      "ListenerName": "AllMessages",
      "FilePath": "C:/SLI/Log/DebugMyThreshold.txt",
      "MinSeverity": "Debug",
      "AmountOfFiles": 100,
      "SizeThresholdInBytes": "20971520"
    },
    "LogError": {
      "Implemented": true,
      "MemorySize": 5000,
      "ListenerName": "ErrorsAndFatalMessagesOnly",
      "FilePath": "C:/SLI/Log/ErrorMyThreshold.txt",
      "MinSeverity": "Error",
      "ImmediateFlush": true
    },
    "LogFatal": {
      "Implemented": false
    },
    "LogInfo": {
      "Implemented": false
    },
    "LogOFF": {
      "Implemented": false
    },
    "LogTrace": {
      "Implemented": false
    },
    "LogTrace_INT": {
      "Implemented": false
    },
    "LogWARN": {
      "Implemented": false
    }
  }
}
